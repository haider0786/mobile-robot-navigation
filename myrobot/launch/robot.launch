<?xml version="1.0" ?>
<launch>
 <param name="/use_sim_time" value="true" /> 
<include file="$(find myrobot)/launch/dy_cr2world.launch"/>
    <arg name="init_pose" value="-x -8.48 -y 0.08 -z 1" />
    <arg name="init_orient" value="-R 0 -P 0 -Y 0.043" />
  
    <param name="robot_description" command="$(find xacro)/xacro.py $(find amr_robots_description)/urdf/pioneer3at_new_laser_rviz.urdf"/>
    	
<node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="$(arg init_pose) $(arg init_orient) -urdf -param robot_description -model" output="screen"/>

   <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher"/>
    <node pkg="robot_state_publisher" type="robot_state_publisher" 
          name="robot_state_publisher" output="screen"/>
 
 
</launch>
